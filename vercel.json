{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/about-us", "dest": "/about-us.html" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/404", "dest": "/404.html" }
  ]
}
