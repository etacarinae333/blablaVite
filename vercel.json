{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/dist/index.html" },
    { "src": "/about-us", "dest": "/dist/about-us.html" },
    { "src": "/contact", "dest": "/dist/contact.html" }
  ]
}
